{
  "name": "ionic-pixlive-module",
  "version": "1.0.0",
  "description": "Ionic module for using the PixLive SDK",
  "main": "dist/bundles/pixlive.umd.min.js",
  "devDependencies": {
    "@angular/common": "~4.1.3",
    "@angular/compiler": "~4.1.3",
    "@angular/compiler-cli": "~4.1.3",
    "@angular/core": "~4.1.3",
    "@angular/forms": "~4.1.3",
    "ionic-angular": "~3.4.2",
    "rollup": "~0.43.0",
    "rxjs": "~5.4.1",
    "typescript": "~2.4.1",
    "uglify-js": "~3.0.20"
  },
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/pixlive.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/pixlive.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "keywords": [
    "augmented reality",
    "ionic",
    "PixLive",
    "Vidinoti"
  ],
  "author": "Johan Leuenberger",
  "license": "MIT"
}